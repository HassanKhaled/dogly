{
  "name": "Dogly",
  "description": "a simple chrome extension that returns a random dog images from remote api",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "manifest_version": 3,

  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
 
   "action": {
   "default_popup": "popup/popup.html"
 }
}
